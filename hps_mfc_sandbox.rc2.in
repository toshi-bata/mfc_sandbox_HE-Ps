
#include "resource.h"

#ifndef APSTUDIO_INVOKED
#include "targetver.h"
#endif
#include "afxres.h"
#include "verrsrc.h"


/////////////////////////////////////////////////////////////////////////////
//
// Version
//

VS_VERSION_INFO     VERSIONINFO
  FILEVERSION         @HPS_VERSION_MAJOR@,@HPS_VERSION_MINOR@,@HPS_VERSION_PATCH@,@HPS_VERSION_BUILD_NUMBER@
  PRODUCTVERSION      @HPS_VERSION_MAJOR@,@HPS_VERSION_MINOR@,@HPS_VERSION_PATCH@,@HPS_VERSION_BUILD_NUMBER@
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x40004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName",        "Tech Soft 3D, Inc."
            VALUE "FileDescription",    "@FILE_DESCRIPTION@"
            VALUE "FileVersion",        "@HPS_VERSION_MAJOR@.@HPS_VERSION_MINOR@.@HPS_VERSION_PATCH@.@HPS_VERSION_BUILD_NUMBER@"
            VALUE "InternalName",       "@TARGET@"
            VALUE "LegalCopyright",     "@COPYRIGHT_STRING@"
            VALUE "OriginalFilename",   "@FILE_NAME@"
            VALUE "ProductName",        "HOOPS Visualize"
            VALUE "ProductVersion",     "@HPS_VERSION_MAJOR@.@HPS_VERSION_MINOR@.@HPS_VERSION_PATCH@.@HPS_VERSION_BUILD_NUMBER@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
